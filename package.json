{
    "name": "@flzyy/create-discord-bot",
    "version": "1.3.1",
    "description": "CLI tool for creating discord bots",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "npx swc src -d dist --config-file .swcrc",
        "prestart": "npm run build",
        "start": "node dist/index.js",
        "lint": "eslint src/**/*.ts",
        "fix": "eslint --fix src/**/*.ts",
		"format": "prettier --write src/**/*",
        "check": "tsc --noEmit",
        "poststart": "rmdir /Q /S dist && rmdir /Q /S bot_dev",
        "patch": "npm version patch && git push --follow-tags",
        "minor": "npm version minor && git push --follow-tags",
        "major": "npm version major && git push --follow-tags"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FLzyy/create-discord-bot.git"
    },
    "keywords": [
        "discordjs",
        "discord.js",
        "javascript",
        "typescript"
    ],
    "author": "FLzyy",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FLzyy/create-discord-bot/issues"
    },
    "homepage": "https://create-discord-bot.github.io/",
    "devDependencies": {
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.3.23",
        "@types/args": "^5.0.0",
        "@types/inquirer": "^9.0.3",
        "@types/node": "^18.11.17",
        "@typescript-eslint/eslint-plugin": "^5.47.0",
        "@typescript-eslint/parser": "^5.47.0",
        "eslint": "^8.29.0",
        "eslint-config-prettier": "^8.5.0",
        "prettier": "^2.8.1",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "giget": "^1.0.0",
        "inquirer": "^9.1.4",
        "mri": "^1.2.0",
        "nanospinner": "^1.1.0"
    },
    "bin": "dist/index.js",
    "files": [
        "dist/**/*",
        "README.md"
    ]
}
