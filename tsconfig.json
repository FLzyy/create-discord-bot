{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"rootDir": "./src",
		"outDir": "./dist"
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"dist",
		"node_modules"
	]
}